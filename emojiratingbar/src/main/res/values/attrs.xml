<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EmojiRatingBar">
        <attr name="showText" format="boolean" />
        <attr name="showAllText" format="boolean" />
        <attr name="titleColor" format="color" />
        <attr name="android:fontFamily"/>
        <attr name="defaultValue" format="enum">
            <enum name="EMPTY" value="0" />
            <enum name="AWFUL" value="1" />
            <enum name="BAD" value="2" />
            <enum name="OKAY" value="3" />
            <enum name="GOOD" value="4" />
            <enum name="GREAT" value="5" />
        </attr>
    </declare-styleable>
</resources>